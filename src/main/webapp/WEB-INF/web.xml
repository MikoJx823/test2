<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SubscribeServlet</servlet-name>
  	<display-name>SubscribeServlet</display-name>
  	<description></description>
  	<servlet-class>com.project.servlet.SubscribeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StringUtil</servlet-name>
  	<display-name>StringUtil</display-name>
  	<description></description>
  	<servlet-class>com.project.util.StringUtil</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SubscribeServlet</servlet-name>
  	<url-pattern>/SubscribeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StringUtil</servlet-name>
  	<url-pattern>/StringUtil</url-pattern>
  </servlet-mapping>
</web-app>
